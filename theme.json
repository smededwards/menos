{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"name": "Deep Black",
					"slug": "deep-black",
					"color": "#19191A"
				},
				{
					"name": "Slate Grey",
					"slug": "slate-grey",
					"color": "#66666B"
				},
				{
					"name": "Charcoal",
					"slug": "charcoal",
					"color": "#424245"
				},
				{
					"name": "Jet Black",
					"slug": "jet-black",
					"color": "#08080A"
				},
				{
					"name": "Soft Gold",
					"slug": "soft-gold",
					"color": "#C29C69"
				},
				{
					"name": "Light Gold",
					"slug": "light-gold",
					"color": "#FFECD2"
				},
				{
					"name": "Warm Gold",
					"slug": "warm-gold",
					"color": "#E8C9A0"
				},
				{
					"name": "Rich Gold",
					"slug": "rich-gold",
					"color": "#A17943"
				},
				{
					"name": "Dark Gold",
					"slug": "dark-gold",
					"color": "#805822"
				},
				{
					"name": "Pure White",
					"slug": "pure-white",
					"color": "#FFFFFF"
				},
				{
					"name": "Soft Silver",
					"slug": "soft-silver",
					"color": "#CECECE"
				},
				{
					"name": "Pale Grey",
					"slug": "pale-grey",
					"color": "#A2A0A0"
				}
			]
		},
		"layout": {
			"contentSize": "640px",
			"wideSize": "1280px"
		},
		"spacing": {
			"units": [ "%", "px", "em", "rem", "vh", "vw" ]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"defaultFontSizes": false,
			"customFontSize": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Noto Sans\", sans-serif",
					"name": "Noto Sans",
					"slug": "noto-sans",
					"fontFace": [
						{
							"fontFamily": "Noto Sans",
							"fontStyle": "normal",
							"fontWeight": "400",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/noto-sans/NotoSans-Regular.woff2"
							]
						},
						{
							"fontFamily": "Noto Sans",
							"fontStyle": "italic",
							"fontWeight": "400",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/noto-sans/NotoSans-Italic.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "\"Noto Serif\", serif",
					"name": "Noto Serif",
					"slug": "noto-serif",
					"fontFace": [
						{
							"fontFamily": "Noto Serif",
							"fontStyle": "normal",
							"fontWeight": "400",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/noto-serif/NotoSerif-Regular.woff2"
							]
						},
						{
							"fontFamily": "Noto Serif",
							"fontStyle": "italic",
							"fontWeight": "400",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/noto-serif/NotoSerif-Italic.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "\"Noto Mono\", monospace",
					"name": "Noto Mono",
					"slug": "noto-mono",
					"fontFace": [
						{
							"fontFamily": "Noto Mono",
							"fontStyle": "normal",
							"fontWeight": "400",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/noto-mono/NotoSansMono-Regular.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "\"Noto Serif Display\", serif",
					"name": "Noto Serif Display",
					"slug": "noto-serif-display",
					"fontFace": [
						{
							"fontFamily": "Noto Serif Display",
							"fontStyle": "normal",
							"fontWeight": "400",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/noto-serif-display/NotoSerifDisplay-Regular.woff2"
							]
						},
						{
							"fontFamily": "Noto Serif Display",
							"fontStyle": "italic",
							"fontWeight": "400",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/noto-serif-display/NotoSerifDisplay-Italic.woff2"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"max": "0.875rem",
						"min": "0.8rem"
					},
					"name": "Extra Small",
					"size": "0.875rem",
					"slug": "x-small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "1rem"
					},
					"name": "Small",
					"size": "1.125rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.25rem",
						"min": "1.125rem"
					},
					"name": "Medium",
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.5rem",
						"min": "1.375rem"
					},
					"name": "Large",
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "1.875rem",
						"min": "1.625rem"
					},
					"name": "Extra Large",
					"size": "1.875rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "2.5rem",
						"min": "2rem"
					},
					"name": "2X Large",
					"size": "2.5rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"max": "3.5rem",
						"min": "2.75rem"
					},
					"name": "3X Large",
					"size": "3.5rem",
					"slug": "xxx-large"
				},
				{
					"fluid": {
						"max": "5rem",
						"min": "3.75rem"
					},
					"name": "4X Large",
					"size": "5rem",
					"slug": "xxxx-large"
				},
				{
					"fluid": {
						"max": "7.125rem",
						"min": "4.25rem"
					},
					"name": "5X Large",
					"size": "7.125rem",
					"slug": "xxxxx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/post-title": {
				"typography": {
					"fontSize": "2rem",
					"lineHeight": "1.2",
					"textAlign": "center"
				}
			},
			"core/preformatted": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-mono)"
				}
			}
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--accent)",
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--accent)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-serif)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-serif-display)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--noto-sans)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "front-page",
			"title": "Front Page",
			"postTypes": [
				"page"
			]
		}
	]
}
